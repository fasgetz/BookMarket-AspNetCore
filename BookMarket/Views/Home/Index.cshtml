@*@model BookMarket.Models.ViewModels.HomeViewModels.IndexViewModel*@

@{
    ViewData["Title"] = "Главная страница";
}

<div id="content">

</div>

@section Scripts {
    <script>

        $(document).ready(function () {
            $.ajax({
                type: "GET",
                url: '/Home/IndexData',

                success: function (data) {
                    $("#content").html(data);
                }
            });
        })

    </script>
}
